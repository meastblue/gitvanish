{
  "nodeModulesDir": "auto",
  "tasks": {
    "dev": "deno run -A npm:astro dev",
    "build": "deno run -A --node-modules-dir=auto npm:astro build",
    "preview": "deno run -A npm:astro preview",
    "astro": "deno run -A npm:astro"
  },
  "imports": {
    "~/": "./",
    "~/assets": "./src/assets",
    "@astrojs/react": "npm:@astrojs/react@^4.4.2",
    "astro": "npm:astro@^5.15.8",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.17",
    "tailwindcss": "npm:tailwindcss@^4.1.17",
    "react": "npm:react@^18.3.1",
    "react-dom": "npm:react-dom@^18.3.1",
    "@types/react": "npm:@types/react@^18.3.12",
    "@types/react-dom": "npm:@types/react-dom@^18.3.1",
    "@octokit/rest": "npm:@octokit/rest@^21.0.2"
  }
}
